{
  "name": "@eventstore/client",
  "version": "1.0.0",
  "description": "TypeScript SDK for GraveyardDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/proto --ts_proto_opt=outputServices=grpc-js,env=node,useOptionals=true,esModuleInterop=true -I ../../proto/api ../../proto/api/eventstore.proto",
    "test": "jest",
    "prepare": "npm run proto:gen && npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "google-protobuf": "^3.21.2",
    "reflect-metadata": "^0.2.1",
    "ts-proto": "^1.166.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}